    "@vitejs/plugin-vue": "^4.0.0",
    "@volar-plugins/vetur": "latest",
    "@vue/eslint-config-prettier": "^7.0.0",
    "bulma": "^0.9.4",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "node-sass": "^8.0.0",
    "prettier": "^2.7.1",
    "vite": "^4.0.0"
  }
